@model Surveyer.Models.EnteringTestModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    if (Model.TestThatUserCanTake.Count == 0)
    {
        <h1>Katılabileceğiniz Test Yok</h1>
    }
    else
    {
        foreach (var item in Model.TestThatUserCanTake)
        {
            <h3>@item.Title</h3>
            @Html.ActionLink("Katıl", "Taking", "TakingSurvey", new { id = item.id }, new { @class = "btn btn-primary" })
        }
    }
}

