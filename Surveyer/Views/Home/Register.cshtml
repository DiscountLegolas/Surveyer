@model Surveyer.Models.LoginAndRegisterModel

@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm())
{
    <h1>Nick</h1>
    @Html.TextBoxFor(x=>x.Nick)
    <h2>Pass</h2>
    @Html.TextBoxFor(x=>x.Pass)
    <button type="submit">Üye Ol</button>
}

